GET         /login               controllers.LoginController.login
GET         /                    controllers.LoginController.login()
POST        /login               controllers.LoginController.loginSubmit

GET         /index                    controllers.LoginController.index()


GET         /showSongForm                    controllers.SongController.showSongForm()
POST        /submitSong                    controllers.SongController.submitSong()

GET         /createperson                controllers.AppUsingMongo.create

POST        /createFromJson              controllers.AppUsingMongo.createFromJson

GET         /findByName/:lastName        controllers.AppUsingMongo.findByName(lastName: String)

GET         /assets/*file        controllers.Assets.versioned(path="/public", file: Asset)

